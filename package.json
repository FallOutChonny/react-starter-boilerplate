{
  "name": "create-universal-react-redux-app",
  "description": "A starter kit for universal react app",
  "keywords": [
    "bootstrap 4",
    "code splitting",
    "es6",
    "express",
    "font-awesome",
    "hot reloading",
    "production optimize",
    "react",
    "react-hot-loader",
    "react-router 4",
    "redux",
    "redux-saga",
    "styled-components",
    "universal",
    "webpack"
  ],
  "version": "0.0.1",
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "chokidar": "^2.0.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "helmet": "^3.9.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.3",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "reactstrap": "next",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.5",
    "styled-components": "^2.4.0",
    "superagent": "^3.8.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "7.1.6",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "bootstrap-loader": "^2.2.0",
    "cache-loader": "^1.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "flow-bin": "^0.57.3",
    "font-awesome-webpack": "0.0.5-beta.2",
    "fs-extra": "4.0.2",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "2.30.1",
    "husky": "^0.14.3",
    "jest": "21.2.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^4.3.0",
    "name-all-modules-plugin": "^1.0.1",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "piping": "^1.0.0-rc.4",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.7.4",
    "promise": "8.0.1",
    "react-dev-utils": "4.1.0",
    "react-error-overlay": "2.0.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "resolve-url-loader": "^2.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-dev-server": "2.9.3",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-isomorphic-tools": "^3.0.5",
    "webpack-manifest-plugin": "1.3.2"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ],
    "*.{json,graphql}": ["prettier --write", "git add"]
  },
  "scripts": {
    "analyze": "node scripts/analyze.js",
    "build": "node scripts/build.js",
    "build:dll": "DLL=true node scripts/buildDLL.js",
    "dev":
      "concurrently --kill-others \"npm run start:dev\" \"npm run watch-client\"",
    "flow": "flow",
    "lint": "eslint -c .eslintrc src",
    "postinstall": "npm run build:dll",
    "postsetup": "npm run build:dll",
    "precommit": "lint-staged",
    "watch-client": "better-npm-run watch-client",
    "start:dev": "better-npm-run start:dev",
    "start:prod": "node server",
    "test": "node scripts/test.js --env=jsdom"
  },
  "betterScripts": {
    "start:dev": {
      "command": "node ./server.js",
      "env": {
        "BABEL_ENV": "development",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node scripts/start.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    }
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx}"],
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "es2015",
      "react-app",
      "stage-0"
    ],
    "plugins": [
      "system-import-transformer",
      [
        "babel-plugin-styled-components",
        {
          "ssr": true
        }
      ],
      [
        "transform-imports",
        {
          "reactstrap": {
            "transform": "react-bootstrap/lib/${member}",
            "preventFullImport": true
          },
          "react-router-dom": {
            "transform": "react-router-dom/${member}",
            "preventFullImport": true
          }
        }
      ],
      "transform-object-rest-spread",
      "transform-export-extensions",
      "react-hot-loader/babel",
      "react-loadable/babel"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dllConfig": {
    "path": "./build/dll",
    "packages": [
      "history",
      "immutable",
      "prop-types",
      "react",
      "react-dom",
      "react-hot-loader",
      "react-loadable",
      "react-redux",
      "react-router",
      "react-router-config",
      "react-router-dom",
      "react-router-redux",
      "reactstrap",
      "redux",
      "redux-devtools",
      "redux-devtools-dock-monitor",
      "redux-devtools-log-monitor",
      "redux-immutable",
      "redux-saga",
      "reselect",
      "styled-components",
      "superagent"
    ]
  },
  "appVendors": [
    "history/createBrowserHistory",
    "immutable",
    "react",
    "react-dom",
    "react-loadable",
    "react-redux",
    "react-router-config",
    "react-router-redux",
    "redux",
    "redux-immutable",
    "redux-saga",
    "reselect",
    "styled-components",
    "superagent"
  ]
}
